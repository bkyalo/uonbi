<footer id="page-footer" data-region="mainpage" class="bg-inverse">
    <div data-region="footer-container-popover">
        {{#output.has_communication_links}}
            <button onclick="window.open('{{output.communication_url}}', '_blank', 'noreferrer')" class="btn btn-icon bg-primary text-white icon-no-margin btn-footer-communication" aria-label="{{#str}}communicationroomlink, course{{/str}}">
                {{#pix}}t/messages-o, core{{/pix}}
            </button>
        {{/output.has_communication_links}}
        <button class="btn btn-icon btn-primary icon-no-margin btn-footer-popover" data-action="footer-popover" aria-label="{{#str}}showfooter, theme_boost{{/str}}">
            {{#pix}}e/question, core{{/pix}}
        </button>
    </div>
    <div class="footer-content-popover container" data-region="footer-content-popover">
        {{#output.has_communication_links}}
            <div class="footer-section p-3 border-bottom footer-link-communication">
                <div class="footer-support-link">{{{ output.communication_link }}}</div>
            </div>
        {{/output.has_communication_links}}
        {{# output.has_popover_links }}
            <div class="footer-section p-3 border-bottom">
                {{# output.page_doc_link }}
                    <div class="footer-support-link">{{{ output.page_doc_link }}}</div>
                {{/ output.page_doc_link }}

                {{# output.services_support_link }}
                    <div class="footer-support-link">{{{ output.services_support_link }}}</div>
                {{/ output.services_support_link }}

                {{# output.supportemail }}
                    <div class="footer-support-link">{{{ output.supportemail }}}</div>
                {{/ output.supportemail }}
            </div>
        {{/ output.has_popover_links }}
        <div class="footer-section p-3 border-bottom">
            <div class="logininfo">
                {{{ output.login_info }}}
            </div>
            <div class="tool_usertours-resettourcontainer">
            </div>

            {{{ output.standard_footer_html }}}
            {{{ output.standard_end_of_body_html }}}
        </div>
        <div class="footer-section p-3">
            <div>{{#str}}poweredbymoodle, core{{/str}}</div>
            {{#output.moodle_release}}
                <div>
                    {{#str}}version, core{{/str}} {{{ output.moodle_release }}}
                </div>
            {{/output.moodle_release}}
        </div>
    </div>
    <div class="container-fluid">
        <div class="footer-lists row">
            {{# footerlinks1 }}
            <div class="col-sm-12 col-md-6 col-lg-3">
                {{# footerlinksheading1}}<h4 class="title">{{footerlinksheading1}}</h4>{{/ footerlinksheading1}}
                <div class="list">{{{footerlinks1}}}</div>
            </div>
            {{/ footerlinks1 }}

            {{# footerlinks2 }}
            <div class="col-sm-12 col-md-6 col-lg-3">
                {{# footerlinksheading2}}<h4 class="title">{{footerlinksheading2}}</h4>{{/ footerlinksheading2}}
                <div class="list">{{{footerlinks2}}}</div>
            </div>
            {{/ footerlinks2 }}

            {{# footerlinks3 }}
            <div class="col-sm-12 col-md-6 col-lg-3">
                {{# footerlinksheading3}}<h4 class="title">{{footerlinksheading3}}</h4>{{/ footerlinksheading3}}
                <div class="list">{{{footerlinks3}}}</div>
            </div>
            {{/ footerlinks3 }}

            {{# footerlinks4 }}
            <div class="col-sm-12 col-md-6 col-lg-3">
                {{# footerlinksheading4}}<h4 class="title">{{footerlinksheading4}}</h4>{{/ footerlinksheading4}}
                <div class="list">{{{footerlinks4}}}</div>
            </div>
            {{/ footerlinks4 }}

        </div>
        <div class="row">
            <div class="col-md-12 col-lg-7">
                {{# footnote }}
                    {{{footnote}}}
                {{/ footnote }}
                {{# contacts }}
                    <div class="contact-info">
                         <p>{{# address}}{{address}}{{/ address}}<br>
                        {{# phoneno}}
                          <i class="fa fa-phone-square"></i> {{phone}} : {{phoneno}}<br>
                        {{/ phoneno}}
                        {{# emailid}}
                          <i class="fa fa-envelope"></i> {{email}} : <a class="mail-link" href="mailto:{{emailid}}">{{emailid}}</a><br>
                        {{/ emailid}}
                        </p>
                    </div>
                {{/ contacts }}
            </div>
            <div class="col-md-12 col-lg-5">
                <div class="copyright ml-auto mb-3 d-lg-flex justify-content-end">
                    {{{copyright}}}
                </div>

                {{# social }}
                <div class="social-media">
                    <ul class="d-lg-flex align-items-center flex-wrap justify-content-end">
                     {{# fburl}}
                        <li class="sm-01"><a href="{{fburl}}" target="_blank"><i class="fa fa-brands fa-facebook"></i></a></li>
                    {{/ fburl}}
                    {{# twurl}}
                        <li class="sm-02"><a href="{{twurl}}" target="_blank"><i class="fa fa-brands fa-x-twitter"></i></a></li>
                    {{/ twurl}}
                    {{# instaurl}}
                        <li class="sm-03"><a href="{{instaurl}}" target="_blank"><i class="fa fa-brands fa-instagram"></i></a></li>
                    {{/ instaurl}}
                    {{# linkedinurl}}
                       <li class="sm-04"><a href="{{linkedinurl}}" target="_blank"><i class="fa fa-brands fa-linkedin"></i></a></li>
                    {{/ linkedinurl}}
                    {{# pinurl}}
                       <li class="sm-05"><a href="{{pinurl}}" target="_blank"><i class="fa fa-brands fa-pinterest"></i></a></li>
                    {{/ pinurl}}
                    {{# youtubeurl}}
                        <li class="sm-07"><a href="{{youtubeurl}}" target="_blank"><i class="fa fa-brands fa-youtube-play"></i></a></li>
                    {{/ youtubeurl}}
                    </ul>
                </div>
                {{/ social }}
            </div>
        </div>

        <div class="footer-content-debugging my-3">
            {{{ output.debug_footer_html }}}
        </div>
    </div>
</footer>

{{#js}}
require(['theme_boost/footer-popover'], function(FooterPopover) {
    FooterPopover.init();
});
{{/js}}

<!-- Modal -->
<div class="modal fade" id="loadingmodal" tabindex="-1" role="dialog" aria-labelledby="loadingmodallabel" aria-hidden="true" data-waiting="{{enablewaiting}}">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <h3>{{# str }} fetching, theme_uonbi {{/ str }}</h3>
                <div class="waitloader"></div>
            </div>
        </div>
    </div>
</div>
